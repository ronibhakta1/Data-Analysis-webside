<!DOCTYPE html>
<html>

<head>
    <title>
        <%= title %>
    </title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.0/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/js/bootstrap.min.js"></script>
</head>

<body>

    <div class="container">
        <div class="row">
            <% computer.forEach(function(row){%>
                <div id="signupbox" style="margin-top:50px"
                    class="mainbox col-md-6 col-md-offset-3 col-sm-8 col-sm-offset-2">
                    <div class="panel panel-info">
                        <div class="panel-heading">
                            <div class="panel-title">
                                <%= title %>
                            </div>
                        </div>
                        <div class="panel-body">
                            <form action="/update8.5/" id="EmployeeForm" class="form-horizontal" method="post"
                                role="form">
                                
                                <div class="form-group">
                                    <label for="student_er_no" class="col-md-3 control-label">ER NO</label>
                                    <div class="col-md-9">
                                        <input type="text" class="form-control" name="student_er_no"
                                            value="<%= row.student_er_no %>" placeholder="Enter ER no">
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label for="water_resource_engineering" class="col-md-3 control-label">water resource engineering</label>
                                    <div class="col-md-9">
                                        <input type="text" class="form-control" name="water_resource_engineering"
                                            value="<%= row.water_resource_engineering %>"
                                            placeholder="water resource engineering">
                                    </div>
                                </div>


                                <div class="form-group">
                                    <label for="design_of_steel_and_rcc_structure" class="col-md-3 control-label">design of steel and rcc
                                        structure</label>
                                    <div class="col-md-9">
                                        <input type="text" class="form-control" name="design_of_steel_and_rcc_structure"
                                            value="<%= row.design_of_steel_and_rcc_structure %>"
                                            placeholder="design of steel and rcc structure">
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label for="estimating_costing_and_valuation" class="col-md-3 control-label">estimating costing and
                                        valuation</label>
                                    <div class="col-md-9">
                                        <input type="text" class="form-control" name="estimating_costing_and_valuation"
                                            value="<%= row.estimating_costing_and_valuation %>"
                                            placeholder="estimating costing and valuation">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="public_health_engineering" class="col-md-3 control-label">public health
                                        engineering</label>
                                    <div class="col-md-9">
                                        <input type="text" class="form-control" name="public_health_engineering"
                                            value="<%= row.public_health_engineering %>"
                                            placeholder="public health engineering">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="traffic_engineering" class="col-md-3 control-label">traffic engineering</label>
                                    <div class="col-md-9">
                                        <input type="text" class="form-control" name="traffic_engineering"
                                            value="<%= row.traffic_engineering %>" placeholder="traffic engineering">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="industrial_training" class="col-md-3 control-label">industrial training</label>
                                    <div class="col-md-9">
                                        <input type="text" class="form-control" name="industrial_training"
                                            value="<%= row.industrial_training %>" placeholder="industrial training">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="capstone_project_planning" class="col-md-3 control-label">capstone project
                                        planning</label>
                                    <div class="col-md-9">
                                        <input type="text" class="form-control" name="capstone_project_planning"
                                            value="<%= row.capstone_project_planning %>"
                                            placeholder="capstone project planning">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="percentage" class="col-md-3 control-label">percentage</label>
                                    <div class="col-md-9">
                                        <input type="text" class="form-control" name="percentage"
                                            value="<%= row.percentage %>" placeholder="percentage">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="rating" class="col-md-3 control-label">rating</label>
                                    <div class="col-md-9">
                                       <input type="text" class="form-control" name="rating"
                                           value="<%= row.rating %>" placeholder="rating">
                                   </div>
                                </div>
                                <div class="form-group">
                                    <label for="remark" class="col-md-3 control-label">remark</label>
                                    <div class="col-md-9">
                                        <input type="text" class="form-control" name="remark" value="<%= row.remark %>"
                                            placeholder="remark">
                                    </div>
                                </div>


                                <div class="form-group">
                                    <!-- Button -->
                                    <div class="col-md-offset-3 col-md-9">
                                        <input type="submit" name="submit" value="submit" class="btn btn-primary">
                                    </div>
                                </div>

                            </form>
                        </div>
                    </div>




                </div>
                <% }) %>
        </div>
    </div>
</body>

</html>