<body>

  <!-- login  -->
  <style>
    .login-form {
      width: 650px;
      margin: 150px auto;
      font-size: 15px;
    }

    .login-form form {
      margin-bottom: 15px;
      background: #f7f7f7;
      box-shadow: 0px 2px 2px rgba(0, 0, 0, 0.3);
      padding: 30px;
    }

    .login-form h2 {
      margin: 110 10 110px;
    }

    .form-control,
    .btn {
      min-height: 38px;
      border-radius: 2px;
    }

    .btn {
      font-size: 15px;
      font-weight: bold;
    }
  </style>
  <div class="login-form">
    <form action="" method="POST">
      <h2 class="text-center">computer sem 6</h2>
      <div class="form-group">
        <label  class="col-md-3 control-label">ER No.</label>
        <input name="student_er_no" class="form-control" placeholder="er no" required="required">
      </div>
      <br>
      <div class="form-group">
        <label  class="col-md-3 control-label">management</label>
        <input  name="management" class="form-control"   placeholder="management" required="required">
      </div>
      <br>
      <div class="form-group">
        <label  class="col-md-3 control-label">contracts and accounts</label>
        <input  name="contracts_and_accounts" class="form-control"   placeholder="contracts and accounts" required="required">
      </div>
      <br>
      <div class="form-group">
        <label  class="col-md-3 control-label">maintenance and repairs of structure</label>
        <input  name="maintenance_and_repairs_of_structure" class="form-control" placeholder="maintenance and repairs of structure" required="required">
      </div>
      <br>
      <div class="form-group">
        <label  class="col-md-3 control-label">emerging trends in civil engineering</label>
        <input  name="emerging_trends_in_civil_engineering" class="form-control"  placeholder="emerging trends in civil engineering" required="required">
      </div>
      <br>
      <div class="form-group">
        <label  class="col-md-3 control-label">solid waste management</label>
        <input name="solid_waste_management" class="form-control" placeholder="solid waste management" required="required">
      </div>
      <br>
      <div class="form-group">
        <label  class="col-md-3 control-label">capstone project execution and report writing</label>
        <input  name="capstone_project_execution_and_report_writing" class="form-control" placeholder="capstone project execution and report writing " required="required">
      </div>
      <br>
      <div class="form-group">
        <label  class="col-md-3 control-label">construction management</label>
        <input  name="construction_management" class="form-control" placeholder="construction management" required="required">
      </div>
      <br>
      <div class="form-group">
        <label  class="col-md-3 control-label">enterprenureship development</label>
        <input  name="enterprenureship_development" class="form-control" placeholder="enterprenureship development" required="required">
      </div>
      <br>
      <div class="form-group">
        <label  class="col-md-3 control-label">percentage</label>
        <input  name="epercentage" class="form-control" placeholder="percentage" required="required">
      </div>
      <br>
      <div class="form-group">
        <label  class="col-md-3 control-label">rating</label>
        <input  name="rating" class="form-control" placeholder="rating" required="required">
      </div>
      <br>
      <div class="form-group">
        <label  class="col-md-3 control-label">remark</label>
        <input  name="remark" class="form-control" placeholder="remark" required="required">
      </div>
      <br>
    
      <br>
      <div class="form-group">
        <button type="submit" value="submit" class="btn btn-primary btn-block">Submit</button>
      </div>

    </form>
    <!-- <p class="text-center"><a href="/homepage">Home</a></p> -->
  </div>

  <!-- login end -->


