<body>

  <!-- login  -->
  <style>
    .login-form {
      width: 650px;
      margin: 150px auto;
      font-size: 15px;
    }

    .login-form form {
      margin-bottom: 15px;
      background: #f7f7f7;
      box-shadow: 0px 2px 2px rgba(0, 0, 0, 0.3);
      padding: 30px;
    }

    .login-form h2 {
      margin: 110 10 110px;
    }

    .form-control,
    .btn {
      min-height: 38px;
      border-radius: 2px;
    }

    .btn {
      font-size: 15px;
      font-weight: bold;
    }
  </style>
  <div class="login-form">
    <form action="/otp11.3" method="POST">
      <h2 class="text-center"> civil sem 3 otp genration</h2>
     
      <div class="form-group">
        <input type="number" name="otp" class="form-control"  placeholder="otp" required="required">
      </div>
     
      <div>
        <label>Select subject</label>
        <select class="form-select" name="subject" aria-label="Default select example">
          <option selected>Select subject</option>
          <option name="advance_surveying">advance_surveying</option>
          <option name="highway_engineering">highway_engineering</option>
          <option name="mechanics_of_structures">mechanics_of_structures</option>
          <option name="building_construction">building_construction</option>
          <option name="concrete_technology">concrete_technology</option>
          <option name="computer_aided_drawing">computer_aided_drawing</option>
        </select>
      </div>
      <br>
      <br>
      <div class="form-group">
        <button type="submit" value="submit" class="btn btn-primary btn-block">Submit</button>
      </div>

    </form>
    <!-- <p class="text-center"><a href="/homepage">Home</a></p> -->
  </div>

  <!-- login end -->


